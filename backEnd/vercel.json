{
    "installCommand": "npm install --legacy-peer-deps",
    "version": 2,
    "builds": [
      { "src": "src/index.js", "use": "@vercel/node" }
    ],
    
    "routes": [
      {
        "src": "/",
        "dest": "/frontEnd/login/home.html"
      },
      {
        "src": "/login",
        "dest": "/frontEnd/login/login.html"
      },
      {
        "src": "/materiRev",
        "dest": "/frontEnd/materiRev/materiRev.html"
      },
      {
        "src": "/bookFis",
        "dest": "/frontEnd/bookFis/bookFis.html"
      },
      {
        "src": "/register",
        "dest": "/frontEnd/login/register .html"
      },
      {
        "src": "/dashboard",
        "dest": "/frontEnd/index.html"
      }
    ]
  }